<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Weggler.SideFragment.RecommendWegglerFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_margin="@dimen/margin_20"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_size_50">
        <TextView
            android:textSize="@dimen/text_size_20"
            android:textStyle="bold"
            android:gravity="center"
            android:text="추천 위글러"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ImageView
            android:id="@+id/back_button_rwf"
            android:layout_marginStart="@dimen/margin_5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:src="@drawable/ic_baseline_arrow_back_ios_24"
            android:layout_width="@dimen/item_size_25"
            android:layout_height="@dimen/item_size_25"
            app:tint="@color/black" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>